{
	"name": "akarrouach-void/php-config-training",
	"description": "Training package - do not use",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": "^8.2",
		"nette/schema": "^1.3",
		"dflydev/dot-access-data": "^3.0"
	},
	"scripts": {
		"lint": "vendor/bin/phpcs --standard=phpcs.xml src",
		"fix-style": "vendor/bin/phpcbf src",
		"analyse": "vendor/bin/phpstan analyse"
	},
	"autoload": {
		"psr-4": {
			"Ansar\\Config\\": "src/"
		}
	},
	"require-dev": {
		"phpstan/phpstan": "^2.1",
		"squizlabs/php_codesniffer": "^4.0"
	}
}
